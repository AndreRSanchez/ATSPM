@model MOE.Common.Business.WCFServiceLibrary.TimingAndActuationsOptions
<div class="card">
    <div class="card-header">Purdue Coordination Diagram Options</div>
    <div class="card-body">

        <div class="">
            @Html.LabelFor(model => model.YAxisMax, new { htmlAttributes = new { @class = "control-label" } })
            @Html.EditorFor(model => model.YAxisMax, new { htmlAttributes = new { @class = "form-control", placeholder = "Auto" } })
        </div>
        <div class="">
            @Html.LabelFor(model => model.Y2AxisMax, new { htmlAttributes = new { @class = "control-label" } })
            @Html.EditorFor(model => model.Y2AxisMax, new { htmlAttributes = new { @class = "form-control", placeholder = "Auto" } })
        </div>
    </div>
</div>